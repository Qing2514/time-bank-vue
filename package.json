{
  "name": "bampo-nuxt",
  "version": "0.0.4",
  "author": "Fortuna",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "preview": "nuxi preview",
    "start": "nuxi start",
    "typecheck": "nuxi typecheck",
    "update": "nuxi upgrade && yarn upgrade-interactive --latest",
    "prepare": "husky install",
    "lint": "yarn typecheck && yarn lint:stylelint && yarn lint:eslint && yarn lint:prettier",
    "lint-staged": "lint-staged --verbose",
    "lint:eslint": "eslint **/*.{tsx,vue,ts,js} --ignore-path .gitignore",
    "lint:eslint:fix": "eslint --fix **/*.{tsx,vue,ts,js} --ignore-path .gitignore",
    "lint:stylelint": "stylelint **/*.sass --ignore-path .gitignore",
    "lint:prettier": "prettier --check **/*.{tsx,vue,ts,js,json,css} --ignore-path .gitignore",
    "lint:prettier:fix": "prettier --write **/*.{tsx,vue,ts,js,json,css} --ignore-path .gitignore",
    "commit": "cz",
    "commit:all": "git add . && cz",
    "commit:amend": "git commit --amend"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-brands-svg-icons": "^5.15.4",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@pinia/nuxt": "^0.1.8",
    "crypto-js": "^4.1.1",
    "pinia": "^2.0.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@commitlint/cz-commitlint": "^16.0.0",
    "@nuxtjs/eslint-config-typescript": "^8.0.0",
    "@types/crypto-js": "^4.1.0",
    "@types/node": "^17.0.8",
    "animated-tailwindcss": "^3.0.0-rc.2",
    "autoprefixer": "^10.4.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-nuxt": "^3.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "inquirer": "^8.2.0",
    "lint-staged": "^12.1.7",
    "nuxt3": "^3.0.0-27356801.e9128f3",
    "postcss": "^8.4.5",
    "postcss-sass": "^0.5.0",
    "prettier": "^2.5.1",
    "rollup": "^2.63.0",
    "sass": "^1.47.0",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.1.0",
    "tailwindcss": "^3.0.13",
    "typescript": "^4.5.4",
    "vue": "^3.2.26",
    "vue-tsc": "^0.30.2"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  }
}
